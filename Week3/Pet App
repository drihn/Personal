public class App {
    public static void main(String[] args) throws Exception {
        pet Pikachu = new pet("Pikachu","Electric type", 7.5F, 10, true);
        Pikachu.petDialogue();

        pet Charmander = new pet("Charmander", "Fire type", 9.9F,10,true );
        Charmander.petDialogue();

        pet Squirtle = new pet("Squirtle","Water type", 8.8F, 9,false);
        Squirtle.petDialogue();

}
}
